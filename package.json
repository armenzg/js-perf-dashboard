{
  "name": "js-perf-dashboard",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:armenzg/js-perf-dashboard.git",
  "author": "Armen Zambrano G. <armenzg@gmail.com>",
  "license": "MPL-2.0",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint --cache --ext mjs,jsx,js --format codeframe \".*.js\" src test",
    "codecov": "codecov && rm -rf coverage"
  },
  "lint-staged": {
    "*.js[x]": [
      "yarn lint"
    ]
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "@material-ui/lab": "3.0.0-alpha.18",
    "chart.js": "^2",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15",
    "query-string": "^6",
    "react": "^16",
    "react-chartjs-2": "^2",
    "react-dom": "^16",
    "react-hot-loader": "^4",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@neutrinojs/airbnb": "9.0.0-rc.4",
    "@neutrinojs/jest": "9.0.0-rc.4",
    "@neutrinojs/react": "9.0.0-rc.4",
    "codecov": "3.6.1",
    "eslint": "6.5.1",
    "fetch-mock": "7.5.1",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "lint-staged": "9.4.1",
    "neutrino": "9.0.0-rc.4",
    "raf": "3.4.1",
    "react-test-renderer": "16.10.2",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  }
}
